<card id="<%= delegator.id %>" size="full-width">
  <card-media>
    <lux-icon-base
      width="50"
      height="50"
      icon-color="rgb(65, 70, 78)"
    ><lux-icon-person></lux-icon-person></lux-icon-base>
  </card-media>
  <card-header>
    <heading level="h2" size="h3"><hyperlink href="<%= assume_delegate_path(delegator) %>"><%= delegator.delegator.full_name %></hyperlink></heading>
    <text-style><%= delegator.delegator.uid %></text-style>
  </card-header>
  <card-content>
      <lux-icon-base icon-name="person">
        <lux-icon-person></lux-icon-person>
      </lux-icon-base>
  </card-content>
</card>
